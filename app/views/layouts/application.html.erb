<!DOCTYPE html>
<html>
  <head>
    <title>Paper</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render('shared/header') %>
    <div id="sidebar">
      <%= render('shared/sidebar') %>
